{
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [{
    "js": ["index.js"],
    "matches": ["*://*.netflix.com/*"],
    "run_at": "document_end"
  }],
  "description": "Blocks the Netflix banner ad on the Netflix main page",
  "icons": {
    "128": "nyan.png"
  },
  "manifest_version": 2,
  "name": "Netflix Banner Blocker",
  "version": "1.0"
}